@model EIP.Entity.RepaymentPlan

@{
    Layout = "~/views/shared/_PersonCenterLayout.cshtml";
}
@section head{
    <link href="/Content/PersonalCenter/CSS/table.css?v=@ViewBag.Version" rel="stylesheet" />
}
<div>
    <div class="div-margin">
        <input type="button" id="Colse" class="form-btn" onclick="closeBox();" value="关闭" />
        
    </div>
    <div class="head-font">
        <b>合同回款计划详情</b>
    </div>

    <table id="readonly" class="tables" cellpadding="0" cellspacing="0">
            
                <tr>
                    <td width="25%">
                        <label>合同基本信息标识</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.RepaymentPlantGUID)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>合同基本信息标识</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.ContractGUID)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>回款计划时间</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.RepaymentPlanDate)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>回款计划金额</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.RepaymentPlanAmount)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>备注</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.Remarks)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>冗余字段</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.Other)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>记录项目的一些基本信息</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.ContractBasicInfo)
                    </td>
                </tr>
            
    </table>
</div>

<script type="text/javascript">

    mini.parse();


    //关闭
    function closeBox() {

        window.App.CloseWindow();
    }
</script>